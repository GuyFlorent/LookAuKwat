                                                    <a href="@Url.Action("HouseDetail", "House", new { id = liste.id }, null)" class="list-group-item">
                                                        <h4 class="list-group-item-heading"> <strong>@liste.Title</strong> Ville:<span class="text-info"><strong> @liste.Town</strong></span></h4>
